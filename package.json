{
	"name": "boilerplate.ts",
	"version": "5.3.3",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"generate-api-specs": "npx openapi-typescript https://api.klickrhein.de/v6/openapi.yaml -o ./src/lib/boilerplate/xioni/api/api.d.ts --make-paths-enum --root-types",
		"postinstall": "npm run generate-api-specs",
		"update": "npx npm-check-updates"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.26.1",
		"@sveltejs/vite-plugin-svelte": "^6.1.0",
		"@types/eslint": "^9.6.1",
		"@typescript-eslint/eslint-plugin": "^8.38.0",
		"@typescript-eslint/parser": "^8.38.0",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"openapi-typescript": "^7.8.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"sass": "^1.89.2",
		"svelte-check": "^4.3.0",
		"sveltekit-autoimport": "^1.8.1",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3",
		"vite": "^7.0.6"
	},
	"type": "module",
	"dependencies": {
		"svelte": "^5.37.0",
		"@types/glidejs__glide": "^3.6.6",
		"@types/lodash-es": "^4.17.12",
		"@glidejs/glide": "^3.7.1",
		"classnames": "^2.5.1",
		"date-fns": "^4.1.0",
		"in-view": "^0.6.1",
		"lodash-es": "^4.17.21",
		"openapi-fetch": "^0.14.0",
		"openapi-typescript-fetch": "^2.2.1",
		"opening_hours": "^3.9.0",
		"svelte-preprocess": "^6.0.3"
	}
}
